@{
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/fonts/icomoon/style.css">
    <link href="~/css/signin/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/css/main/bootstrap.css" rel="stylesheet" />
    <link href="~/css/main/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/signin/style.css" rel="stylesheet" />
    <link href="~/css/sign.css" rel="stylesheet" />
    <link href="~/css/main/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/main/prettyPhoto.css" rel="stylesheet" />
    <link href="~/css/main/animate.css" rel="stylesheet" />
    <link href="~/css/main/mainMeow.css" rel="stylesheet" />
    <link href="~/css/main/responsive.css" rel="stylesheet" />
    <link href="~/css/main/Themes.css" rel="stylesheet" />
    <title>Регистрация</title>
</head>
<body>
    <div class="">
        <div class="">
            <div class="container">
                <div class="row">
                    <div class="col-12 cd-lg-10" style="text-align: center;">
                        <h1 style="margin-left: 0px;">Регистрация</h1>
                        <p>Если вы уже создали аккаунт. <a href="#">Авторизируйтесь</a></p>
                    </div>
                </div>
                <div class="row justify-content-center">

                    <div class="col-10">
                        <form class="row" style="margin-bottom: 5px;">
                            <fieldset>
                                <div class="form-group col-10 cd-2">
                                    <label for="exampleInputEmail1" class="form-label mt-4">Эл.Почта</label>
                                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="example.mail.ru">
                                    <small id="emailHelp" class="form-text text-muted">Никому не сообщайте свою почту для авторизации</small>
                                </div>
                                <div class="form-group col-10 cd-2">
                                    <label for="exampleInputEmail1" class="form-label mt-4">Повторите Эл.почту</label>
                                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="example.mail.ru">
                                </div>
                                <div class="form-group col-10 cd-2">
                                    <label for="exampleInputPassword1" class="form-label mt-4">Придумайте пароль</label>
                                    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="***********">
                                </div>
                                <div class="form-group col-10 cd-2">
                                    <label for="exampleInputPassword1" class="form-label mt-4">Павторите пароль</label>
                                    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="************">
                                </div>

                                <button type="submit" class="btn btn-outline-info" style="outline: none; box-shadow: none; width: auto; height: auto;">Зарегистрироваться</button>
                            </fieldset>
                        </form>
                    </div>

                    <div class="col-10 align-self-center social-login">
                        <a href="#" style="outline: none; box-shadow: none; width: auto; height: auto;" class="facebook btn d-flex justify-content-center align-items-center">
                            <span style="margin-right: 10px;" class="icon-facebook mr-3"></span> Login with Facebook
                        </a>
                        <a href="#" style="outline: none; box-shadow: none; width: auto; height: auto;" class="twitter btn d-flex justify-content-center align-items-center">
                            <span style="margin-right: 10px;" class="icon-twitter mr-3"></span> Login with  Twitter
                        </a>
                        <a href="#" style="outline: none; box-shadow: none; width: auto; height: auto;" class="google btn d-flex justify-content-center align-items-center">
                            <span style="margin-right: 10px;" class="icon-google mr-3"></span> Login with  Google
                        </a>
                    </div>
                </div>

            </div>
        </div>


    </div>



    <script src="~/js/main/jquery.js"></script>
    <script src="~/js/main/jquery.scrollUp.min.js"></script>
    <script src="~/js/main/bootstrap.min.js"></script>
    <script src="~/js/main/jquery.prettyPhoto.js"></script>
    <script src="~/js/main/main.js"></script>
    <script src="~/js/main/Themes.js"></script>
</body>
</html>